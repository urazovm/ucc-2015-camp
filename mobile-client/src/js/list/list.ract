<div class="home background"></div>
<div class="container home">
    <div class="card card-container">


        <h1>Items</h1>

        <div class="row">
            {{#each estimationSession.items}}
            <div class="col-xs-12">
                <h2>{{.item.name}}</h2>
                <p>{{.item.description}}</p>
                <div class="btn btn-primary pull-right" on-click='viewItem:{{.item}}'>Estimate</div>
            </div>
            {{/each}}
        </div>

    </div>
</div>